{
  "name": "mui-language-picker",
  "version": "1.0.5",
  "description": "Material UI language picker",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist src/index coverage",
    "copyCss": "copyfiles --flat src/LanguagePicker.css dist",
    "index": "node src/langPicker/makeIndexes.js",
    "build": "npm run index && npm test && tsc && npm run copyCss",
    "lint": "tslint -c tslint.json 'src/**/*.{ts.tsx}'",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sillsdev/mui-language-picker.git"
  },
  "keywords": [
    "language",
    "bcp47",
    "font",
    "script"
  ],
  "author": "Greg Trihus <greg_trihus@sil.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sillsdev/mui-language-picker/issues"
  },
  "homepage": "https://github.com/sillsdev/mui-language-picker#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.152",
    "@types/node": "^12.12.42",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "copyfiles": "^2.3.0",
    "jest": "^26.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5",
    "write": "^2.0.0"
  },
  "peerdependencies": {
    "@material-ui/core": "^4.10.0",
    "@material-ui/icons": "^4.9.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|.*\\.test)\\.(ts|tsx)$"
  }
}
